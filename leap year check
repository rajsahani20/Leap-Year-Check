#include<stdio.h>
int main(){
    int year;
    printf("Enter The Year To Check :");
    scanf("%d",&year);
    
    if ((year % 400 == 0) || (year % 4 == 0 && year % 4 != 0)){
        printf("%d is a leap year.\n", year);
    }else{
        printf("%d is not a leap year.\n", year);
    }
    
    return 0;
}
